<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RadarView">

        <attr name="edgeNum" format="integer" />
        <attr name="level" format="integer" />
        <attr name="levelColor" format="color" />
        <attr name="radiusLineColor" format="color" />
        <attr name="angelNameTextSize" format="dimension" />
        <attr name="horizontalTextSpace" format="dimension" />
        <attr name="vertex" format="boolean" />
        <attr name="chartFillColor" format="color" />
        <attr name="debug" format="boolean" />
        <attr name="maxProgress" format="integer" />
        <attr name="pointColor" format="color" />
        <attr name="pointRadius" format="dimension" />
        <attr name="chartStrokeColor" format="color" />
    </declare-styleable>

    <declare-styleable name="BaseShadowView">
        <attr name="tshadowDx" format="dimension" />
        <attr name="tshadowDy" format="dimension" />
        <attr name="tshadowColor" format="color" />
        <attr name="tshadowRadius" format="dimension" />
        <attr name="topenShadow" format="boolean" />

    </declare-styleable>

    <declare-styleable name="BaseMaskFilterView">

        <attr name="openMaskFilter" format="boolean" />
        <attr name="maskRadius" format="dimension" />
        <attr name="maskColor" format="color" />
        <attr name="maskFilterBlurType">
            <enum name="INNER" value="1" />
            <enum name="SOLID" value="2" />
            <enum name="NORMAL" value="3" />
            <enum name="OUTER" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ShadowLayout">
        <attr name="shadowDx" format="dimension" />
        <attr name="shadowDy" format="dimension" />
        <attr name="shadowColor" format="color" />
        <attr name="shadowRadius" format="dimension" />
        <attr name="shadowPosition">
            <enum name="all" value="1" />
            <enum name="left" value="2" />
            <enum name="top" value="3" />
            <enum name="right" value="4" />
            <enum name="bottom" value="5" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ItgHorizontalProgressBar">
        <attr name="iBarBackgroundColor" format="color" />
        <attr name="iBarColor" format="color" />
        <attr name="iMax" format="integer" />
        <attr name="iProgress" />
        <attr name="iCap" format="enum">
            <enum name="rect" value="0" />
            <enum name="round" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SlideMenu">
        <attr name="iMenuMarginRight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MyTextView">
        <attr name="iTextSize" format="dimension" />
        <attr name="iTextColor" format="color" />
        <attr name="iText" format="string" />
    </declare-styleable>

    <declare-styleable name="RingProgressBar">

        <attr name="irBorderWidth" format="dimension" />
        <attr name="irOutColor" format="color" />
        <attr name="irInColor" format="color" />
        <attr name="irTextSize" format="dimension" />
        <attr name="irMaxProgressValue" format="integer" />
        <attr name="irProgress" format="integer" />
        <attr name="irTextColor" format="color" />

    </declare-styleable>

    <declare-styleable name="HorProgressView">
        <attr name="hpBackground" format="color" />
        <attr name="hpColor" format="color" />
        <attr name="hpTextColor" format="color" />
        <attr name="hpTextSize" format="dimension" />
        <attr name="hpCap" format="boolean" />
        <attr name="hpProgress" format="integer" />
        <attr name="hpMaxProgress" format="integer" />
    </declare-styleable>

    <declare-styleable name="RatingBarView">

        <attr name="rbRatingNum" format="integer" />
        <attr name="rbGrade" format="integer" />
        <attr name="rbZeroGradeIcon" format="reference" />
        <attr name="rbGradeIcon" format="reference" />

    </declare-styleable>

    <declare-styleable name="LetterSideView">

        <attr name="lsTextSize" format="dimension" />
        <attr name="lsTextColor" format="color" />
        <attr name="lsLightTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="My_AppBarLayout_Layout">
        <attr name="my_layout_scrollFlags">
            <flag name="my_scroll" value="0x1" />
            <flag name="my_exitUntilCollapsed" value="0x2" />
            <flag name="my_enterAlways" value="0x4" />
            <flag name="my_enterAlwaysCollapsed" value="0x8" />
            <flag name="my_snap" value="0x10" />
            <flag name="my_snapMargins" value="0x20" />
        </attr>
        <attr name="my_layout_scrollInterpolator" format="reference" />
    </declare-styleable>

    <declare-styleable name="MyAppBarLayout">
        <attr name="my_elevation" />
        <attr name="android:background" />
        <attr name="my_expanded" format="boolean" />
        <attr name="android:keyboardNavigationCluster" />
        <attr name="android:touchscreenBlocksFocus" />
        <attr name="my_liftOnScroll" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MyScrollingViewBehavior">
        <attr name="my_behavior_overlapTop" format="dimension" />

    </declare-styleable>


    <declare-styleable name="MyAppBarLayoutStates">
        <attr name="my_state_collapsed" format="boolean" />
        <attr name="my_state_collapsible" format="boolean" />
        <attr name="my_state_lifted" format="boolean" />
        <attr name="my_state_liftable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SimpleBarLayout_Layout">
        <attr name="simple_bar_layout_scrollFlags">
            <flag name="bar_scroll" value="0x1" />
            <flag name="bar_exitUntilCollapsed" value="0x2" />
            <flag name="bar_enterAlways" value="0x4" />
            <flag name="bar_enterAlwaysCollapsed" value="0x8" />
            <flag name="bar_snap" value="0x10" />
        </attr>

        <attr name="bar_layout_scrollInterpolator" format="reference" />
    </declare-styleable>

    <declare-styleable name="SimpleBarLayout">
        <attr name="sbl_elevation" />
        <attr name="android:background" />
        <attr name="sbl_expanded" format="boolean" />
        <attr name="android:keyboardNavigationCluster" />
        <attr name="android:touchscreenBlocksFocus" />
        <attr name="liftOnScroll" format="boolean" />
    </declare-styleable>


    <declare-styleable name="NestedScrollBehavior_Layout">
        <attr name="nsb_behavior_overlapTop" format="dimension" />
    </declare-styleable>
</resources>